&GLOBAL
  PROJECT cage 
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
  WALLTIME 86300
&END GLOBAL

!&EXT_RESTART
!  RESTART_FILE_NAME ./amides-1.restart
!  RESTART_POS
!  RESTART_VEL
!  RESTART_AVERAGES
!  RESTART_CELL
!  RESTART_THERMOSTAT
!  RESTART_COUNTERS
!&END

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME ./BASIS_MOLOPT
    POTENTIAL_FILE_NAME ./GTH_POTENTIALS
    CHARGE 12 
    &MGRID 
      CUTOFF 400
      REL_CUTOFF 40
      NGRIDS 5
    &END 
    &QS
      EPS_DEFAULT 1.0E-12
      METHOD GPW
      EXTRAPOLATION ASPC
      EXTRAPOLATION_ORDER 3
    &END QS
    &SCF
      MAX_SCF 50
      EPS_SCF 1.0E-6
      SCF_GUESS ATOMIC
      &OT 
        MINIMIZER DIIS
        N_DIIS 7
        PRECONDITIONER FULL_SINGLE_INVERSE
      &END OT
      &OUTER_SCF
        EPS_SCF 1.0E-6
        MAX_SCF 10
      &END OUTER_SCF
    &END SCF
    &XC
      &XC_FUNCTIONAL BLYP
      &END XC_FUNCTIONAL
      &VDW_POTENTIAL
         POTENTIAL_TYPE PAIR_POTENTIAL
         &PAIR_POTENTIAL
            SCALING 0.75
            TYPE DFTD2
            R_CUTOFF [angstrom] 20
         &END PAIR_POTENTIAL
      &END VDW_POTENTIAL
    &END XC
    &POISSON
       PERIODIC NONE
       PSOLVER MT
    &END
  &END DFT
 &SUBSYS
    &CELL
      ABC [angstrom] 50.0 50.0 50.0
      ALPHA_BETA_GAMMA 90.000000 90.000000 90.000000
    &END CELL
    &TOPOLOGY
      COORDINATE XYZ
      COORD_FILE_NAME ./last.xyz
    &END TOPOLOGY
    &KIND N
      BASIS_SET TZVP-MOLOPT-GTH
      POTENTIAL GTH-BLYP-q5
    &END KIND
    &KIND Pd
      BASIS_SET DZVP-MOLOPT-SR-GTH 
      POTENTIAL GTH-BLYP-q18 
    &END KIND
    &KIND H
     BASIS_SET TZVP-MOLOPT-GTH
     POTENTIAL GTH-BLYP-q1
     MASS 2.01410175
    &END KIND
    &KIND C
     BASIS_SET TZVP-MOLOPT-GTH 
     POTENTIAL GTH-BLYP-q4
    &END KIND
 &END SUBSYS
  &PROPERTIES
    &RESP
      STRIDE 1
      &SPHERE_SAMPLING
        AUTO_VDW_RADII_TABLE CAMBRIDGE
        AUTO_RMIN_SCALE 1.0
        AUTO_RMAX_SCALE 3.0
      &END
      &PRINT
       &COORD_FIT_POINTS
       &END
       &V_RESP_CUBE
       &END 
      &END
    &END
  &END
&END FORCE_EVAL

